
  <% @incoming.each do |user| %>
  <% @users.each do |f| %>
  <% if f.id == user.user.id %>
  <ul>
    New friend requests
  <li>
    <%= f.name %>
    <%= link_to "Accept request", friend_request_path(id: user), :method => :put %>
  </li>
  <% end %>
  <% end %>
  <% end %>
</ul>

<ul>
	All users
  <% @users.each do |user| %>
  <% if user.id != current_user.id %>
  <li>
    <%= user.name %>
 <%= link_to "Add Friend", friend_requests_path(:friend_id => user), method: :post %></li>
</li>
   <% end %>
   <% end %>
 </ul>