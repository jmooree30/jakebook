
<% if !user_signed_in? %>
<%= render 'devise/sessions/new' %>
<%= render 'devise/registrations/new' %>
<% else %>

<% @posts.each do |post| %>
<h4><%= post.body %><br>
</h4>
<%= link_to like_post_path(post), method: :put do %>
Like
<%= post.get_upvotes.size %>
<% end %>

<%= link_to dislike_post_path(post), method: :put do %>
Dislike
<%= post.get_downvotes.size %>
<% end %>
<% end %>

<% end %>